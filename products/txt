def scrape_website(website):
    print("launchin chrome browser...")

    options = webdriver.ChromeOptions()
    driver = webdriver.Chrome(
        service=Service(ChromeDriverManager().install()), options=options
    )

    try:
        driver.get(website)
        driver.implicitly_wait(5.0)

        input_element = driver.find_element(By.ID, value="fi-q")
        input_element.clear()
        search_items = [
            "Computing",
            "Electronics",
            "Sporting Goods",
            "Phones & Tablets",
            "Toys & Games",
            "Automobile",
            "Health & Beauty",
            "Industrial & Scientific",
            "Fashion",
        ]
        for search_item in search_items:
            try:
                input_element = WebDriverWait(driver, 10).until(
                    EC.element_to_be_clickable((By.ID, "fi-q"))
                )
                input_element.clear()
                input_element.send_keys(search_item)

                # Wait for the submit button to be clickable
                submit_button = WebDriverWait(driver, 10).until(
                    EC.element_to_be_clickable((By.CLASS_NAME, "_prim"))
                )
                submit_button.click()

                driver.implicitly_wait(5.0)

                # Wait for results to load
                time.sleep(3)

                soup = BeautifulSoup(driver.page_source, "html.parser")

                results_container = soup.find(id="jm")

                if results_container:
                    results = results_container.find('main', class_='has-b2top')

                    if results:
                        print('hello world!')
                    for result in results:
                        title = result.find('h3', class_='name').get_text(strip=True) if result.find('h3', class_='name') else None
                        image_url = result.find('img')['src'] if result.find('img') else None
                        product_url = result.find('a', href=True)['href'] if result.find('a', href=True) else None
                        product_price = result.find('div', class_='prc').get_text(strip=True) if result.find('div', class_='prc') else None

                    #     print(title, image_url, product_price, product_url)



            except StaleElementReferenceException:
                print(
                    f"Element reference was stale. Retrying for search item: {search_item}"
                )
                continue

    except Exception as e:
        raise e

    finally:
        driver.quit()


scrape_website("https://www.jumia.com.ng/")


reactor my code and get the correct title, image_url, product_url, and product_price